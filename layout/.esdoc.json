{
  "source": "./dist/docs",
  "destination": "./generated-docs",
  "title": "ugLayout Documentation",
  "manual": {
    "configuration": ["./manual/Configuring Renderables.md"]
  },
  "plugins": [{
    "name": "esdoc-importpath-plugin",
    "option": {
      "replaces": [
        { "from": "^docs\\/", "to": "" },
        { "from": "\\/esdoc\\.js", "to": "" }
      ]
    }
  }]
}